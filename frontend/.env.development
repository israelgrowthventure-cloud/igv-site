# Enable/Disable features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PDF_DOWNLOAD=true
REACT_APP_ENABLE_PDF_EMAIL=true

# Public URL
PUBLIC_URL=/
